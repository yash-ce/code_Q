#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;
int main()
{
  int t;
  cin>>t;
  while(t--){
    int n;
    cin>>n;
    //cout<<t<<" "<<n<<endl;
    int a[n][n];
    for(int i=0;i<n;i++){
      for(int j=0;j<n;j++){
        	cin>>a[i][j];
      }
    }
    int i = 0, j =0 ;
    for(i=0;i<n;i++){
      for(j=n-1;j>0;j--){
        	cout<<a[j][i]<<" ";
      }
      cout<<a[0][i];
      cout<<endl;
    }
    cout<<endl;
  }
   return 0;
}

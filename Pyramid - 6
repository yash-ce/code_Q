#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;
int main()
{
  int n;
  cin>>n;
  for(int i=1;i<=n;i++){
    if(i == 1){
      cout<<1;
    }
    else{
      for(int j=i;j>0;j--){
        cout<<j;
      }
      for(int j=2;j<=i;j++){
        cout<<j;
      }
    }
    cout<<endl;
  }
   return 0;
}
